<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shiqkuangsan.mycustomviews">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name=".MyApplication"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activity.SwitchPictureActivity" />
        <activity android:name=".ui.activity.MyOnOffActivity"/>
        <activity android:name=".ui.activity.PullRefreshActivity"/>
        <activity android:name=".ui.activity.SlideMenuActivity"/>
        <activity android:name=".ui.activity.PtrDemoActivity"/>
        <activity android:name=".ui.activity.PicChoserActivity"/>
        <activity android:name=".ui.activity.PicLookerActivity"/>
        <activity android:name=".ui.activity.RealmStudyActivity"/>

    </application>

</manifest>